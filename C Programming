Given an array of 1's and 0's which has all 1's first followed by all o's.Find the number of 0's.
testcase 1 :arr[]={1,0,0,0,0}
#include <stdio.h>

int main()
{
    int arr[]={1,0,0,0,0};
    int count=0,i;
    for(i=0;i<=5;i++)
    {
    if(arr[i]==0)
    {
        count++;
    }
    }
      printf("%d",count);
  

    return 0;
}

testcase 2: arr[]={1,1,1,1,0,0}
#include <stdio.h>

int main()
{
    int arr[]={1,1,1,1,0,0};
    int count=0,i;
    for(i=0;i<=5;i++)
    {
    if(arr[i]==0)
    {
        count++;
    }
    }
      printf("%d",count);
  

    return 0;
}
testcase 3 :arr[]={0,0,0}
#include <stdio.h>

int main()
{
    int arr[]={0,0,0};
    int count=0,i;
    for(i=0;i<=5;i++)
    {
    if(arr[i]==0)
    {
        count++;
    }
    }
      printf("%d",count);
  

    return 0;
}
testcase 4:arr[]={1,1,1,1}
#include <stdio.h>

int main()
{
    int arr[]={1,1,1,1};
    int count=0,i;
    for(i=0;i<=5;i++)
    {
    if(arr[i]==0)
    {
        count++;
    }
    }
      printf("%d",count);
  

    return 0;
}



